{
  "name": "ksuid-generator",
  "displayName": "KSUID Generator",
  "description": "Generate KSUID",
  "version": "0.0.1",
  "license": "MIT",
  "author": {
    "name": "Vicente Gonz√°lez",
    "email": "vicente19600@gmail.com"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "keywords": [
    "KSUID"
  ],
  "activationEvents": [
    "onCommand:ksuid.generate",
    "onCommand:ksuid.repeat"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ksuid.generate",
        "title": "Generate KSUID"
      },
      {
        "command": "ksuid.repeat",
        "title": "Repeat KSUID"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "ksuid": "^3.0.0",
    "tslint": "^6.1.3"
  }
}
