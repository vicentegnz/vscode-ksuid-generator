{
  "name": "ksuid-generator",
  "displayName": "KSUID Generator",
  "description": "Generate KSUID",
  "version": "0.0.3",
  "license": "MIT",
  "publisher": "vicentegnz",
  "icon": "docs/ksuid.png",
  "author": {
    "name": "Vicente Gonz√°lez",
    "email": "vicente19600@gmail.com"
  },
  "modulePaths": [
    "./out/test/runTest.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vicentegnz/vscode-ksuid-generator.git"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "keywords": [
    "KSUID"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onCommand:ksuid.generate",
    "onCommand:ksuid.repeat"
  ],
  "contributes": {
    "commands": [
      {
        "command": "ksuid.generate",
        "title": "Generate KSUID"
      },
      {
        "command": "ksuid.repeat",
        "title": "Repeat KSUID"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorTextFocus",
          "command": "ksuid.generate",
          "group": "ksuid"
        },
        {
          "when": "editorHasMultipleSelections",
          "command": "ksuid.repeat",
          "group": "ksuid"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "test": "node ./out/test/runTest.js",
    "deploy": "vsce publish --yarn"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "typescript": "^4.5.5",
    "vsce": "^2.7.0"
  },
  "dependencies": {
    "ksuid": "^3.0.0"
  }
}
